{
  "name": "guofsyrians-slider-backend",
  "version": "1.0.0",
  "description": "Hono backend for Guofsyrians Slider with Cloudflare D1",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "db:migrate": "wrangler d1 migrations apply guofsyrians-db",
    "db:migrate:local": "wrangler d1 migrations apply guofsyrians-db --local",
    "db:seed": "wrangler d1 execute guofsyrians-db --file=./migrations/seed.sql",
    "db:seed:local": "wrangler d1 execute guofsyrians-db --local --file=./migrations/seed.sql",
    "test:api": "bash scripts/test-api.sh",
    "setup": "bash scripts/setup.sh"
  },
  "dependencies": {
    "hono": "^4.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241127.0",
    "wrangler": "^3.87.0"
  }
}
